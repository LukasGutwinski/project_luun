<%= simple_form_for(@listing) do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :title %>
  <%= f.input :mileage %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :year %>
  <%= f.input :condition %>
  <%= f.input :origin %>
  <%= f.input :city %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit %>
<% end %>
