<ul>
  <% @listings.each do |listing| %>
    <li>
    <%= link_to listing.title, listing_path(listing) %>
 - Price: <%= listing.price %> - <%=  listing.mileage %>
    <%= link_to "Delete", listing_path(listing),
      method: :delete,
      data: { confirm: "Are you sure?" } %> - <%= link_to "Edit", edit_listing_path(listing) %></li>
  <% end %>
</ul>

<%= link_to "Add new listing", new_listing_path %>
