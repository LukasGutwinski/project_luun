$("#listing-<%= j @favorite.listing.id.to_s %>-hidden").show();
$("#listing-<%= j @favorite.listing.id.to_s %>-hidden").parent().attr("href", '/listings/<%= j @favorite.listing.id.to_s %>/favorites/<%= j (@favorite.id).to_s %>');
